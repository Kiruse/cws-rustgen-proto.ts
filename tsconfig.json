{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "downlevelIteration": true,
    "strict": true,
    "outDir": "dist",
    "rootDir": "src",
  },
  "include": ["src/**/*", "types/**/*"],
  "exclude": ["node_modules"],
}
